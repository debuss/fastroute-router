{
  "name": "debuss-a/fastroute-router",
  "description": "A PSR-15 router implementation using FastRoute as the routing engine.",
  "license": "MIT",
  "keywords": [
    "fastroute",
    "fast-route",
    "psr",
    "psr-15",
    "router"
  ],
  "authors": [
    {
      "name": "Alexandre Debussch√®re",
      "email": "alexandre.debusschere@hey.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "nikic/fast-route": "^1.3",
    "psr/http-server-middleware": "^1.0",
    "psr/http-factory": "^1"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "pestphp/pest": "^4.3",
    "mockery/mockery": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "Router\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
